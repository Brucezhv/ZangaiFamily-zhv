/**index.wxss**/
.container {
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
} 

.head-page {
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  height: 40%;
  align-items: center;
  background-color: #00ffff;
}

.head-page1 {
  display: flex;
  position: absolute;
  height: 40%;
  top: 40%;
  left: 0;
  bottom: 0;
  right: 0;
  align-items: center;
  background-color: #008080;
}


.image-page {
  display: inline-flex;
  position: absolute;
  width: 50%;
  height: 350rpx;
  margin-left: 50rpx;
  justify-content: center;
  align-items: center;
}

.head-right-page {
  position: absolute;
  width: 40%;
  right: 0;
}

.head-title-page {
  display: flex;
  font-size: 25px;
  width: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.head-r-page {
  color: #00ff00;
  font-size: 20px;
}

.ll {
    display: flex;
  position: absolute;
  height: 20%;
  top: 80%;
  left: 0;
  bottom: 0;
  right: 0;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border: 1px solid #aaa;
  
}