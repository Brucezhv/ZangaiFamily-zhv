/**index.wxss**/
.swiper-page {
  width: 750rpx;
  height: 350rpx;
}
.swiper-item-page {
  display: flex;
  width: 100%;
  height: 100%;
}
.image-swiper {
  width: 100%;
  height: 100%;
}
.choice-page {
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  width: 100%;
  height: 90rpx;
}
.choice-true-page {
  font-size: 40rpx;
  padding-bottom: 5rpx;
  border-bottom: 8rpx solid red;
}
.choice-false-page {
  font-size: 32rpx;
}
.footer-page {
  width: 100%;
}
.footer-head-page {
  display: inline-flex;
  margin: 0 36rpx;
  margin-top: 28rpx;
  height: 272rpx;
  box-shadow: 0 0 8px 1px rgb(190, 190, 190);
}
.footer-head-img-page {
  width: 272rpx;
  height: 272rpx;
}
.footer-right-page {
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 400rpx;
  height: 100%;
}
.footer-right-price-page {
  display: inline-block;
}
.footer-right-font-page {
  display: inline-block;
  color: red;
}
.page-vip-page {
  padding: 0 36rpx;
  width: 678rpx;
  padding-bottom: 28rpx;
}
.choice-show-page {
  display: inline-flex;
  justify-content: space-between;
  align-items: center;
}
.choice-img-page {
  width: 300rpx;
  height: 300rpx;
}
.page-left-page {
  display: inline-flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 320rpx;
  height: 400rpx;
  position: relative;
  margin-top: 28rpx;
  box-shadow: 0 0 8px 1px #c0c0c0;
}
.page-right-page {
  margin-left: 26rpx;
  display: inline-flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 320rpx;
  height: 400rpx;
  position: relative;
  margin-top: 28rpx;
  box-shadow: 0 0 8px 1px #c0c0c0;
}
.font-choice-style-page {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
  width: 100%;
}
.ll {
  display: flex;
  width: 100%;
  justify-content: space-between;
}
.ee {
  font-size: 15px;
}
.aa {
  color: #c0c0c0;
}
