/**index.wxss**/
.confirm {
  background-color: #efeff5;
  width: 100%;
}
.confirm-page {
  width: 672rpx;
}
.main-page {
  margin-top: 15rpx;
  border-radius: 22rpx;
  background-color: #fff;
  border: 1px solid #efeff5;
}
.head-page {
  display: inline-flex;
  height: 120rpx;
  position: relative;
}
.head-img {
  margin-left: 22rpx;
  margin-right: 22rpx;
  width: 118rpx;
  height: 120rpx;
}
.font-head-page {
  display: flex;
  width: 500rpx;
  flex-direction: column;
  justify-content: space-around;
  position: relative;
}
.font-head-tit-page {
  display: flex;
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 30rpx;
}
.font-head-text-page {
  display: flex;
  margin-bottom: 30rpx;
  font-size: 22rpx;
  color: #666;
}
.upload-page {
  height: 280rpx;
  padding: 20rpx 15rpx;
  padding-right: 22rpx;
  border-radius: 22rpx;
  background-color: #fff;
  border: 1px solid #efeff5;
}
.upload-head-page {
  position: relative;
}
.upload-head-choice-page {
  display: inline-flex;
  position: absolute;
  right: 0;
}
.choice-center {
  color: #333;
  margin-left: 22rpx;
  font-size: 30rpx;
  padding: 10rpx 26rpx;
  border: 1px solid #ccc;
  border-radius: 30rpx;
}
.choice-hover-img {
  color: #fe8c48;
  margin-left: 22rpx;
  background-color: #fff4ee;
  font-size: 30rpx;
  padding: 10rpx 26rpx;
  border: 1px solid #fea976;
  border-radius: 30rpx;
}
.upload-center-page {
  margin-top: 20rpx;
  position: relative;
}
.upload-center-number-page {
  display: inline-flex;
  position: absolute;
  right: 0;
}
.upload-center-left-page {
  width: 50rpx;
}
.upload-center-right-page {
  width: 50rpx;
}
.upload-center-center-page {
  width: 70rpx;
  text-align: center;
  color: #333;
  background-color: #efeff5;
}
.upload-footer-page {
  position: relative;
  margin-top: 20rpx;
}
.upload-footer-left {
  display: inline-flex;
  position: absolute;
  color: #333;
}
.upload-footer-right {
  display: inline-flex;
  position: absolute;
  right: 0;
  color: #ccc;
}
.upload-footer-img-page {
  position: absolute;
  margin-top: 60rpx;
  right: 0;
}
.upload-footer-img-left {
  width: 80rpx;
  height: 80rpx;
}
.upload-footer-img-right {
  width: 80rpx;
  height: 80rpx;
  margin-left: 20rpx;
}
.say-page {
  padding: 20rpx 15rpx;
  padding-right: 22rpx;
  height: 260rpx;
  position: relative;
}
.say-head-page {
  display: inline-flex;
  position: absolute;
  justify-content: space-between;
  width: 52%;
  right: 22rpx;
}
.choice-hover-true-say {
  color: #fff;
  background-color: #fe8c48;
  font-size: 26rpx;
  padding: 10rpx 16rpx;
  border: 1px solid #fe8c48;
  border-radius: 26rpx
}
.choice-hover-false-say {
  color: #fff;
  font-size: 26rpx;
  padding: 10rpx 16rpx;
  background-color: #ccc;
  border: 1px solid #ccc;
  border-radius: 35rpx;
}
.check-hover-false-say {
  color: #fe8c48;
  font-size: 26rpx;
  padding: 10rpx 16rpx;
  background-color: #fff;
  border: 1px solid #fea26a;
  border-radius: 35rpx;
}
.say-center-page {
  display: inline-flex;
  position: absolute;
  right: 22rpx;
  left: 15rpx;
  justify-content: space-between;
  margin-top: 65rpx;
}
.say-footer-page {
  display: inline-flex;
  position: absolute;
  right: 22rpx;
  margin-top: 130rpx;
  justify-content: space-between;
  left: 15rpx;
}
.say-footer {
  display: inline-flex;
  position: absolute;
  bottom: 15rpx;
  align-items: center;
  left: 15rpx;
}
.say-footer-explain {
  width: 420rpx;
  font-size: 28rpx;
}
.count-page {
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  padding: 20rpx 15rpx;
  padding-right: 22rpx;
  height: 180rpx;
 
}
.count-head-page{
  position: relative;
}
.count-head-switch {
  right: 0;
  position: absolute;
}
.count-footer-page {
  position: relative;
}
.count-footer-switch {
  right: 0;
  position: absolute;
}
.footer-page {
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-shadow: 0px 0px 8px 1px #dedee4;
  width: 100%;
  height: 100rpx;
  margin-top: 20rpx;
  background-color: #fff;
  position: relative;
}
.final-money-page {
  margin-left: 78rpx;
  color: #ef5242;
}
.count-final-page {
  display: flex;
  position: absolute;
  right: 0;
  width: 40%;
  height: 100%;
  background-color: #ff4d3a;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.wx-switch-input{width:42px !important;height:20px !important;}
.wx-switch-input::before{width:41px !important;height: 20px !important;}
.wx-switch-input::after{width: 18px !important;height: 18px !important;}