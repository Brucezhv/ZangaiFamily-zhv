/**index.wxss**/
.container {
  border-top: 1px solid #c0c0c0;
  padding: 0px 54rpx;
}
.head-page {
  font-size: 34rpx;
  color: #6e6e6e;
  padding-top: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center; 
}
.gfd_index_tit {
  width: 40%;
}
.head-tit-page {
  margin-top: 30rpx;
}
.dog {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.gfd_index_dogHead {
  width: 50%;
  margin-top: 50rpx;
}
.gfd_index_dogFoot {
  bottom: -10rpx;
  width: 80%;
  position: absolute;
}
.input-box {
  border-radius: 15px;  
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  box-shadow: 0rpx 0rpx 50rpx -15rpx #e3907f;
}
.gfd_index_redBorder {
  display: inline-flex;  
}
.input-border-tit {
  display: inline-flex;
  position: absolute;
}
.input-center-page {
  position: absolute;
  width: 330rpx;
  height: 80rpx;
  margin-top: 50rpx;
  align-items: center;
  background-color: #fae5df;
  border-radius: 18px;
  text-indent: 15px;
  display: flex;
}
.input-center {
  margin-left: 30rpx;
}